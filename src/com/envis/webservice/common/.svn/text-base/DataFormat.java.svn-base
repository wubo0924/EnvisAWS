package com.envis.webservice.common;

public enum DataFormat {
	JSON (1), CSV (2);
    
	private int index; 

	private DataFormat(int idx) { 
        this.index = idx; 
    } 

    public int getIndex() { 
        return index; 
    }
    public String toString() {

        return String.valueOf(this.index);

    }
}
