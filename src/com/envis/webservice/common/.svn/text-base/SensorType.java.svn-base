package com.envis.webservice.common;

public enum SensorType {
	TEMPERATURE(1), LIGHT(2), SOUND(3), MOTION(4), HUMIDITY(5), QUALITY(6), WATERLEVEL(7),OTHER(8);
	private int index;
    private SensorType(int index){
    	this.index=index;
    }
    public int getIndex() { 
        return index; 
    } 
    public String toString() {

        return String.valueOf(this.index);

    }
}
